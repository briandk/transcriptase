{
  "name": "transcriptase",
  "productName": "Transcriptase",
  "version": "0.3.1",
  "description": "A Github Electron app for creating and editing Transdown files (briandk.com/transdown)",
  "main": "main/index.js",
  "author": {
    "name": "Brian Danielak",
    "email": "briandaniela.k+github@gmail.com",
    "url": "http://briandk.com/transcriptase"
  },
  "scripts": {
    "dev": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist -- --dir -c.compression=store -c.mac.identity=null"
  },
  "build": {
    "appId": "com.episteme.transcriptase",
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "zip"
      ],
      "description": "",
      "icon": "build/icon.iconset"
    },
    "mac": {
      "target": [
        "dmg"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/briandk/transcriptase.git"
  },
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/briandk/transcriptase/issues"
  },
  "homepage": "https://github.com/briandk/transcriptase#readme",
  "dependencies": {
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "luxon": "^1.0.0",
    "moment": "^2.22.0",
    "quill": "^1.3.5",
    "source-map-support": "^0.5.5",
    "uikit": "^3.0.0-beta.39",
    "video.js": "^6.2.4"
  },
  "devDependencies": {
    "7zip-bin": "^3.0.0",
    "@types/luxon": "^0.5.0",
    "@types/node": "^10.0.8",
    "@types/quill": "^1.3.6",
    "@types/video.js": "^6.2.4",
    "@types/webpack": "^4.1.3",
    "awesome-typescript-loader": "^5.0.0",
    "devtron": "^1",
    "electron": "2.0.0",
    "electron-builder": "^20.8.1",
    "electron-webpack": "^2.0.1",
    "electron-webpack-ts": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "inline-source-map": "^0.6.2",
    "jasmine": "^3.0.0",
    "prettier": "^1.10.2",
    "source-map": "^0.7.0",
    "source-map-loader": "^0.2.3",
    "spectron": "^3.7.2",
    "tslint": "^5.9.1",
    "tslint-loader": "^3.6.0",
    "typescript": "^2.7.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14"
  },
  "browser": {
    "fs": false
  }
}
