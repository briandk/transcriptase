{
  "compilerOptions": {
    "target": "es2017",
    "module": "commonjs",
    "moduleResolution": "node",
    "allowJs": true,
    "jsx": "react",
    "experimentalDecorators": true,
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "strictNullChecks": false,
    "noImplicitReturns": true,
    "noImplicitAny": true
  },
  "include": ["src", "./storybook/views", "test"],
  "exclude": ["node_modules", "src/renderer/prism-markdown", "dist"]
}
